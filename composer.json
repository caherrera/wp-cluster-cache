{
  "name": "caherrera/wp-cluster-cache",
  "type": "library",
  "description": "Wordpress Plugin that cleans cache files to other servers from a cluster",
  "keywords": [
    "performance"," caching"," wp-cache"," wp-super-cache"," cache"
  ],
  "license": "MIT",
  "support": {
    "docs": "https://github.com/caherrera/wp-cluster-cache/blob/master/readme.md",
    "issues": "https://github.com/caherrera/wp-cluster-cache/issues",
    "source": "https://github.com/caherrera/wp-cluster-cache",
    "email": "caherrera@gmail.com"
  },
  "authors": [
    {
      "name": "Carlos Herrera",
      "email": "caherrera@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~1.0"
  },
  "suggest": {
    "ext-fileinfo": "fileinfo is required when retrieving user encoded thumbnails"
  },
  "archive": {
    "exclude": ["/examples", "/tests"]
  },
  "autoload": {
    "psr-4": {
      "WpClusterCache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WpClusterCache\\Tests\\": "tests/"
    }
  }
}
